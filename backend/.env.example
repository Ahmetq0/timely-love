# Copy this file to .env and fill in your values

# Firebase Admin SDK - get from Firebase Console > Project Settings > Service Accounts
# Download the JSON key file and either:
# Option A: Put the path to the JSON file below
FIREBASE_SERVICE_ACCOUNT_PATH=./serviceAccountKey.json

# Option B: Or paste the JSON content as a single line in FIREBASE_SERVICE_ACCOUNT_JSON (not recommended for production)

# JWT (use a long random string in production)
JWT_SECRET=your-jwt-secret-change-in-production

# Server
PORT=3001
